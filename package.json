{
    "name": "talktalk",
    "version": "1.7.0",
    "license": "UNLICENSED",
    "private": true,
    "sasslintConfig": "conf/sass-lint.yml",
    "eslintConfig": {
        "extends": [
            "./conf/eslint/eslint.js"
        ]
    },
    "engines": {
        "node": ">= 7"
    },
    "scripts": {
        "postinstall": "rm -rf ./dist && yarn webdriver-manager",
        "dev": "set NODE_ENV=development && yarn grunt webpack && concurrently --raw --kill-others \"webpack-dev-server -d\" \"yarn watch\"",
        "devBuild": "yarn grunt webpack && yarn webpack -d",
        "karma": "karma",
        "watch": "yarn grunt webpack && yarn karma start ./karma.conf.js",
        "unit": "yarn watch --single-run",
        "unitAfterBuild": "yarn karma start ./conf/karma/karma.afterBuild.js",
        "prod": "rm -rf ./dist && export NODE_ENV=production && yarn grunt webpack && yarn webpack -p --optimize-minimize",
        "webpack-dev-server": "webpack-dev-server",
        "grunt": "grunt",
        "e2e": "source ./scripts/gitlab-ci/protractor_helper.sh && yarn protractor e2e/protractor_local.js",
        "e2e_ci": "yarn protractor e2e/protractor.js",
        "protractor": "protractor",
        "webpack": "webpack",
        "webdriver-manager": "webdriver-manager update --chrome",
        "bower": "bower",
        "bower-update": "bower update",
        "cypress": "cypress open",
        "e2e-RPRC": "cypress run --project ./src/experiments",
        "cypress-run": "cypress run",
        "make-component": "sh ./scripts/make-component/start.sh"
    },
    "devDependencies": {
        "babel-core": "^6.24.1",
        "babel-loader": "^7.0.0",
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-preset-es2015": "^6.24.1",
        "bower": "^1.8.2",
        "concurrently": "^3.4.0",
        "css-loader": "^0.28.0",
        "cypress": "^1.1.3",
        "eslint": "^3.19.0",
        "eslint-loader": "^1.7.1",
        "extract-text-webpack-plugin": "^2.1.0",
        "global": "^4.3.2",
        "grunt": "^1.0.1",
        "grunt-contrib-imagemin": "^1.0.1",
        "grunt-html2js": "^0.3.8",
        "grunt-include-source": "^1.1.0",
        "grunt-ng-constant": "^2.0.1",
        "grunt-sonar-runner": "^2.4.8",
        "grunt-wiredep": "^3.0.1",
        "html-webpack-plugin": "^2.28.0",
        "imports-loader": "^0.7.1",
        "istanbul-instrumenter-loader": "^2.0.0",
        "jasmine-spec-reporter": "^4.2.1",
        "karma": "^1.6.0",
        "karma-coverage": "^1.1.1",
        "karma-fixture": "^0.2.6",
        "karma-jasmine": "^1.1.0",
        "karma-json-fixtures-preprocessor": "0.0.6",
        "karma-junit-reporter": "^1.2.0",
        "karma-phantomjs-launcher": "^1.0.4",
        "karma-webpack": "^2.0.3",
        "load-grunt-tasks": "^3.5.2",
        "ng-annotate-loader": "^0.6.0",
        "null-loader": "^0.1.1",
        "pixrem": "^3.0.2",
        "postcss-loader": "^2.0.3",
        "protractor": "^5.2.2",
        "raw-loader": "^0.5.1",
        "sass-loader": "^6.0.3",
        "source-map-loader": "^0.2.1",
        "style-loader": "^0.17.0",
        "webpack": "^2.4.1",
        "webpack-dev-server": "^2.4.2",
        "wiredep": "^4.0.0"
    },
    "dependencies": {
        "@uirouter/angularjs": "latest",
        "angular": "^1.6.4",
        "angular-animate": "^1.6.4",
        "angular-cookies": "^1.6.4",
        "angular-loading-bar": "^0.9.0",
        "angular-mocks": "^1.6.4",
        "angular-sanitize": "^1.6.4",
        "angular-ui-bootstrap": "^2.5.0",
        "angular-ui-mask": "^1.8.7",
        "babel-polyfill": "^6.26.0",
        "bootstrap-sass": "^3.3.7",
        "contentful": "^4.5.0",
        "crypto-js": "^3.1.9-1",
        "jquery": "^3.1.1",
        "lodash.filter": "^4.6.0",
        "lodash.find": "^4.6.0",
        "lodash.first": "^3.0.0",
        "lodash.head": "^4.0.1",
        "lodash.isempty": "^4.4.0",
        "lodash.some": "^4.6.0",
        "lodash.sortby": "^4.7.0",
        "lodash.values": "^4.3.0",
        "ngmap": "^1.18.4",
        "node-sass": "^4.5.3"
    }
}
